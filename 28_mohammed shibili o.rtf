\ulnone\b0 package com.ajil.myapplication;\par
\par
import androidx.appcompat.app.AppCompatActivity;\par
import androidx.fragment.app.FragmentManager;\par
\par
import android.os.Bundle;\par
import android.view.View;\par
import android.widget.Button;\par
\par
public class MainActivity extends AppCompatActivity \{\par
\par
    @Override\par
    protected void onCreate(Bundle savedInstanceState) \{\par
        super.onCreate(savedInstanceState);\par
        setContentView(R.layout.activity_main);\par
        FragmentManager fragmentManager = getSupportFragmentManager();\par
\par
        Button btnbook = findViewById(R.id.b1);\par
        btnbook.setOnClickListener(new View.OnClickListener() \{\par
            @Override\par
            public void onClick(View view) \{\par
                fragmentManager.beginTransaction()\par
                        .replace(R.id.fragmentContainerView3 , booking.class , null)\par
                        .setReorderingAllowed(true)\par
                        .addToBackStack("name")\par
                        .commit();\par
            \}\par
        \});\par
        Button btnprice = findViewById(R.id.b2);\par
        btnprice.setOnClickListener(new View.OnClickListener() \{\par
            @Override\par
            public void onClick(View view) \{\par
                fragmentManager.beginTransaction()\par
                        .replace(R.id.fragmentContainerView3 , price.class , null)\par
                        .setReorderingAllowed(true)\par
                        .addToBackStack("name")\par
                        .commit();\par
            \}\par
        \});\par
\par
    \}\par
\}\ul\b\par
